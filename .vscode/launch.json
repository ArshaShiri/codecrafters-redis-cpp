{
    "configurations": [
        {
            "name": "Debug Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/src/redis/test/redis_tests",
            "args": [
                "--gtest_filter=RedisTest.setAndGetCommand"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ],
    "version": "2.0.0"
}
